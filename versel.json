{
  "buildCommand": "CI=false npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "vite",
  "devCommand": "npm run dev",
  "regions": ["iad1"],
  "functions": {
    "maxDuration": 60
  },
  "env": {
    "NODE_ENV": "production",
    "CI": "false"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ]
}